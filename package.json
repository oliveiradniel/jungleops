{
  "name": "fullstack-challenge",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run start:dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/passport-jwt": "^4.0.1",
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@9.1.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/throttler": "^6.4.0",
    "cookie-parser": "^1.4.7",
    "passport-jwt": "^4.0.1"
